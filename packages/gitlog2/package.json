{
  "name": "gitlog2",
  "version": "5.0.10",
  "description": "Git log parser for Node.JS",
  "keywords": [
    "git",
    "log",
    "parser"
  ],
  "homepage": "https://github.com/bluelovers/ws-git-lazy/tree/master/packages/gitlog2#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-git-lazy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-git-lazy.git"
  },
  "license": "ISC",
  "author": "Dom Harrington",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc npm run test",
    "lint": "npx eslint **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run ncu && yarn run sort-package-json",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx sort-package-json ./package.json"
  },
  "dependencies": {
    "@git-lazy/spawn": "^1.0.11",
    "array-hyper-unique": "^1.1.2",
    "bluebird": "^3.7.2",
    "crlf-normalize": "^1.0.3",
    "cross-spawn-extra": "^2.0.0",
    "debug": "^4.1.1",
    "decamelize": "^4.0.0",
    "git-decode": "^1.0.5",
    "lodash.assign": "^4.2.0",
    "sort-object-keys2": "^2.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/debug": "^4.1.5"
  },
