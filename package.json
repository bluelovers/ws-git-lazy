{
  "name": "ws-git-lazy",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/@git-lazy/*",
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lerna-publish": "lerna publish --yes --cd-version patch",
    "ncu": "npx yarn-tool ncu -u && npx yarn-tool ws exec yarn-tool ncu -- -u",
    "run-workspace-publish": "yarn run workspace-publish",
    "run-workspace-version": "yarn run workspace-version",
    "sort-package-json": "npx yarn-tool sort && npx yarn-tool ws exec yarn-tool sort"
  },
  "resolutions": {},
  "dependencies": {
    "workspace-release": "^0.5.1"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/jest": "^25.2.3",
    "@types/node": "latest",
    "@types/yargs": "^15.0.5",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0"
  }
}
